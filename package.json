{
  "name": "@ideallabs/etf.js",
  "version": "0.0.5-dev",
  "description": "a typescript sdk for encryption/decryption with the etf network",
  "license": "GPL-3.0",
  "repository": "https://github.com/ideal-lab5/etf.js",
  "main": "dist/etf.js",
  "dependencies": {
    "@ideallabs/connect": "^0.0.1-dev",
    "@ideallabs/etf-sdk": "^0.0.3-dev",
    "@polkadot/api": "^10.9.1",
    "@polkadot/api-augment": "^10.9.1",
    "@polkadot/extension-dapp": "^0.46.3",
    "@polkadot/rpc-provider": "^10.9.1",
    "@polkadot/types": "^10.9.1",
    "@polkadot/util": "^12.0.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "//node_modules/(?!variables/.*)"
    ],
    "moduleNameMapper": {
      "@ideallabs/etf-sdk": "<rootDir>/__mocks__/@ideallabs/etf-sdk.ts",
      "@polkadot/rpc-provider": "<rootDir>/__mocks__/@polkadot/rpc-provider.ts",
      "@polkadot/api": "<rootDir>/__mocks__/@polkadot/api.ts",
      "@polkadot/api-augment": "<rootDir>/__mocks__/@polkadot/api-augment.ts",
      "@polkadot/types": "<rootDir>/__mocks__/@polkadot/types.ts"
    }
  },
  "scripts": {
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.22.11",
    "@jest/globals": "^29.6.4",
    "@types/jest": "^29.5.4",
    "jest": "^29.6.2",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.1.1",
    "ts-jest-resolver": "^2.0.1"
  }
}
